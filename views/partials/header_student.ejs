<body class="bg-light">

  <% if(typeof id !== 'undefined' && typeof fullName !== 'undefined'){%>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a href="/StudentHomepage?id=<%=id%>&fullName=<%=fullName%>"><img src="imgs/samilogonew.png" alt="logo" class='logo'></a>
        
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
      
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav mr-auto">
            <a class="nav-link" href="/logout">LogOut <span class="sr-only">(current)</span></a>
              <a class="nav-link" href="/StudentHomepage?id=<%=id%>&fullName=<%=fullName%>">Home <span class="sr-only">(current)</span></a>
              <a class="nav-link" href="/studentUserProfile?id=<%=id%>&fullName=<%=fullName%>">Profile <span class="sr-only">(current)</span></a>

            </ul>
      </nav>
    <% } %>